<?xml version="1.0" ?>
<robot name="onrobot_2fg7" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find grippers_lanited_description)/urdf/materials.xacro" />

  <xacro:macro name="onrobot_2fg7_gripper" params="prefix">
    <link name="${prefix}base_link">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="1.2646523179240972"/>
        <inertia ixx="0.001504" iyy="0.001812" izz="0.001053" ixy="-0.0" iyz="0.0" ixz="-3e-06"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://grippers_lanited_description/meshes/base_link.STL" />
        </geometry>
        <material name="silver"/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://grippers_lanited_description/meshes/base_link.STL" />
        </geometry>
      </collision>
    </link>

    <link name="${prefix}right_finger_link">
      <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
        <mass value="0.305"/>
        <inertia ixx="1.25" iyy="0.27" izz="1.21" ixy="-0.34" iyz="0.425" ixz="0.13"/>
      </inertial>
      <visual>
        <origin xyz="0.0 0.0 -0.0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://grippers_lanited_description/meshes/finger_link.STL"/>
        </geometry>
        <material name="silver"/>
      </visual>
      <collision name="right_finger_collision">
        <origin xyz="0.0 0.0 -0.0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://grippers_lanited_description/meshes/finger_link.STL"/>
        </geometry>
      </collision>
    </link>

    <link name="${prefix}left_finger_link">
      <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
        <mass value="0.305"/>
        <inertia ixx="-1.25" iyy="-0.27" izz="-1.21" ixy="0.34" iyz="-0.425" ixz="-0.13"/>
      </inertial>
      <visual>
        <origin xyz="-0.0 0.0 -0.0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://grippers_lanited_description/meshes/finger_link.STL"/>
        </geometry>
        <material name="silver"/>
      </visual>
      <collision name="left_finger_collision">
        <origin xyz="0.0 0.0 -0.0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://grippers_lanited_description/meshes/finger_link.STL"/>
        </geometry>
      </collision>
    </link>

    <joint name="${prefix}left_finger_joint" type="prismatic">
      <origin xyz="0 0.0902 0.11065" rpy="0 0 3.1416"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}left_finger_link"/>
      <axis xyz="0.0 1.0 0.0"/>
      <limit lower="0.0" upper="0.019" effort="140" velocity="0.45"/> 
      <!-- 0.38 -->
    </joint>

    <joint name="${prefix}right_finger_joint" type="prismatic">
      <origin xyz="0.0 -0.0902 0.11065" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}right_finger_link"/>
      <axis xyz="0.0 1.0 0.0"/>
      <limit lower="-0.019" upper="0.019" effort="140" velocity="0.45"/>
      <mimic joint="${prefix}left_finger_joint" multiplier="1.0"/>
    </joint>
    
    <xacro:include filename="$(find grippers_lanited_description)/urdf/onrobot_2fg7.trans.xacro" />
    <xacro:gripper_transmission prefix="${prefix}"/>
    
    <xacro:include filename="$(find grippers_lanited_description)/urdf/onrobot_2fg7.gazebo" />
    <xacro:gazebo_control prefix="${prefix}" />

  </xacro:macro>

</robot>
