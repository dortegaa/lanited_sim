<launch>

  <arg name="initial_joint_positions" doc="Initial joint configuration of the robot"
       default=" -J elbow_joint 0 -J shoulder_lift_joint 0 -J shoulder_pan_joint 0 -J wrist_1_joint 0 -J wrist_2_joint 0 -J wrist_3_joint 0"/>


  <!-- send robot urdf to param server -->
  <param name="robot_description" command="xacro  '$(find ur_description)/urdf/ur5_robot.urdf.xacro'" />

   <!-- push robot_description to factory and spawn robot in gazebo at the origin, change x,y,z arguments to spawn in a different position -->
  <arg name="world_pose" value="-x 0 -y 0 -z 0.1" />
  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model robot3 $(arg world_pose) $(arg initial_joint_positions)"
    respawn="false" output="screen" />
</launch>
