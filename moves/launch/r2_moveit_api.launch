<launch>

  <node name="r2_moveit_api" pkg="moves" type="r2_moveit_api" respawn="false" output="screen">
    <rosparam command="load" file="$(find ur_gripper_moveit_config)/config/kinematics.yaml"/>
    <param name="/planning_plugin" value="ompl_interface/OMPLPlanner"/>
    <rosparam command="load" file="$(find ur_gripper_moveit_config)/config/ompl_planning.yaml"/>
  </node>

</launch>