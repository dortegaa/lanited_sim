<?xml version="1.0"?>

<launch>
 
    <include file="$(find cell_sim)/launch/launch_gazebo.launch"/>

        <group ns="robot1">
        
        <param name="robot_description" command="$(find xacro)/xacro '$(find ur_gripper_sim)/urdf/ur5_robotiq_gripper.xacro'" />
        <param name="tf_prefix" value="robot1_tf" />

         <include file="$(find cell_sim)/launch/spawn_model.launch">
                <arg name="robot_name" value="robot1"/>
                <arg name="init_pose" value="-x -0.983 -y -0.189 -z 1.05"/>
                <arg name="robot_joint_pos" 
                        value="
                        -J shoulder_pan_joint -1.57 
                        -J shoulder_lift_joint 0 
                        -J elbow_joint -1.57 
                        -J wrist_1_joint -1.57 
                        -J wrist_2_joint -1.57 
                        -J wrist_3_joint 0 
                        "/>
        </include>
       <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
            <param name="publish_frequency" type="double" value="50.0" />
        </node>

        <include file="$(find ur_gripper_sim)/launch/launch_controller_ur5_robotiq.launch"/>
        <node name = "rqt_joint_trajectory_controller" pkg = "rqt_joint_trajectory_controller" type = "rqt_joint_trajectory_controller" />
        <!--node name = "rqt_publisher" pkg = "rqt_publisher" type = "rqt_publisher"/-->

    </group>

    <group ns="robot2">
        
        <param name="robot_description" command="$(find xacro)/xacro '$(find ur_gripper_sim)/urdf/ur5_robotiq_gripper.xacro'" />
        <param name="tf_prefix" value="robot2_tf" />

         <include file="$(find cell_sim)/launch/spawn_model.launch">
                <arg name="robot_name" value="robot2"/>
                <arg name="init_pose" value="-x 1.186 -y -0.405 -z 1.05"/>
                <arg name="robot_joint_pos" 
                        value="
                        -J shoulder_pan_joint -1.57 
                        -J shoulder_lift_joint 0 
                        -J elbow_joint -1.57 
                        -J wrist_1_joint -1.57 
                        -J wrist_2_joint -1.57 
                        -J wrist_3_joint 0 
                        "/>
        </include>
       <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
            <param name="publish_frequency" type="double" value="50.0" />
        </node>

        <include file="$(find ur_gripper_sim)/launch/launch_controller_ur5_robotiq.launch"/>
        <node name = "rqt_joint_trajectory_controller" pkg = "rqt_joint_trajectory_controller" type = "rqt_joint_trajectory_controller" />
        <!--node name = "rqt_publisher" pkg = "rqt_publisher" type = "rqt_publisher"/-->

    </group>

    <group ns="mesa">
        
        <param  name="robot_description" 
                textfile="$(find mesa_general)/urdf/mesa_general.urdf" />
        <param  name="tf_prefix" 
                value="model1_tf" />

        <include file="$(find cell_sim)/launch/spawn_model.launch">
                <arg name="robot_name" value="mesa"/>
                <arg name="init_pose" value="-x 0 -y 0 -z 0.09"/>
                
        </include>

        <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
            <param name="publish_frequency" type="double" value="50.0" />
        </node>
  
    </group>


 
</launch>