<?xml version="1.0"?>
<launch>
 
    
    <!-- joint_state_controller -->
    <rosparam file="$(find cell_sim)/config/joint_state_controller.yaml" command="load"/>
    <node name="joint_state_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn joint_state_controller" respawn="false" output="screen"/>
  

    <rosparam file="$(find cell_sim)/config//cell_controllers.yaml" command="load"/>
    <node name="arm_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn r1/arm_controller r2/arm_controller r3/arm_controller"  respawn="false" output="screen"/>

   
    <rosparam file="$(find cell_sim)/config//gripper_controller_robotiq.yaml" command="load"/> 
    <node name="gripper_controller_spawner" pkg="controller_manager" type="spawner" args="r1/gripper r2/gripper r3/gripper"/>
  

</launch>